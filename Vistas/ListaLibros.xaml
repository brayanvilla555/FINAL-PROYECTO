<UserControl x:Class="FINAL_PROYECTO.Vistas.ListaLibros"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:FINAL_PROYECTO.Vistas"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">

    <UserControl.Resources>
        <DataTemplate DataType="{x:Type local:RegistrarLibro}">
            <local:RegistrarLibro DataContext="{Binding}" />
        </DataTemplate>
    </UserControl.Resources>

    <Grid Background="Aquamarine">
        <Button Content="Button" Click="Button_Click" Margin="320,208,382,202" />
    </Grid>
</UserControl>